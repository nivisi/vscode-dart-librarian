{
  "name": "dart-librarian",
  "displayName": "dart-librarian",
  "description": "Export dart files to libraries at ease!",
  "version": "1.0.0",
  "publisher": "nivisi",
  "repository": {
    "type": "git",
    "url": "https://github.com/nivisi/dart-librarian"
  },
  "author": {
    "name": "Nikita Sirovskiy"
  },
  "homepage": "https://github.com/nivisi/dart-librarian",
  "bugs": {
    "url": "https://github.com/nivisi/dart-librarian/issues"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "dart",
    "flutter",
    "library",
    "package export",
    "export"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dart-librarian.export",
        "title": "Export to Library",
        "category": "Dart Librarian"
      },
      {
        "command": "dart-librarian.removeExport",
        "title": "Remove Export",
        "category": "Dart Librarian"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceExtname == .dart && resourcePath =~ /lib/",
          "command": "dart-librarian.export",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .dart && resourcePath =~ /lib/",
          "command": "dart-librarian.removeExport",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceExtname == .dart && resourcePath =~ /lib/",
          "command": "dart-librarian.export",
          "group": "1_dart_librarian"
        },
        {
          "when": "resourceExtname == .dart && resourcePath =~ /lib/",
          "command": "dart-librarian.removeExport",
          "group": "1_dart_librarian"
        }
      ],
      "editor/context": [
        {
          "when": "resourceExtname == .dart && resourcePath =~ /lib/",
          "command": "dart-librarian.export",
          "group": "1_dart_librarian"
        },
        {
          "when": "resourceExtname == .dart && resourcePath =~ /lib/",
          "command": "dart-librarian.removeExport",
          "group": "1_dart_librarian"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}